<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="autoScrolling" format="boolean" />
    <attr name="expanded" format="boolean" />
    <attr name="radius" format="float" />
    <attr name="typeface" format="string" />

    <declare-styleable name="AutoCompleteTextView">
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="Button">
        <attr name="autoScrolling" />
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="CheckBox">
        <attr name="autoScrolling" />
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="EditText">
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="GridView">
        <attr name="expanded" />
    </declare-styleable>
    <declare-styleable name="ImageView">
        <attr name="radius" />
        <attr name="scaleType">
            <flag name="leftCrop" value="0x01" />
            <flag name="topCrop" value="0x02" />
            <flag name="rightCrop" value="0x04" />
            <flag name="bottomCrop" value="0x08" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ListView">
        <attr name="expanded" />
    </declare-styleable>
    <declare-styleable name="RadioButton">
        <attr name="autoScrolling" />
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="TextView">
        <attr name="autoScrolling" />
        <attr name="typeface" />
    </declare-styleable>

</resources>